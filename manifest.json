{
  "name": "Degiro statistics",
  "description": "Shows interesting portfolio statistics at the website of broker Degiro.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "action": {},
  "content_scripts": [{
    "matches": ["*://trader.degiro.nl/*"],
    "js": ["main.js"]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
